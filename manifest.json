{
  "manifest_version": 3,
  "name": "Sort Bookmarks by Theme",
  "description": "Sort your existing bookmarks into folders by theme",
  "version": "1.0",
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  }

}
